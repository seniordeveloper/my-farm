{
    "eslint.enable": true,
    "eslint.format.enable": true,
    "eslint.validate": [
        "javascript",
        "typescript",
        "html"
    ],
    "eslint.options": {
        "extensions": [
            ".js",
            ".ts",
            "html"
        ]
    },
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "editor.insertSpaces": false,
    "editor.tabSize": 4,
    "editor.detectIndentation": true,
    "editor.autoIndent": "full",
    "typescript.tsdk": "node_modules\\typescript\\lib"
}